{
    "batch_size": 64,
    "num_workers": 4,
    "embedding_dim": 128,
    "d_input": 32,
    "event_length": 256,
    "n_layers": 6,
    "n_heads": 4,
    "output_dim": 3,
    "n_epochs": 200,
    "patience": 200,
    "dropout": 0.1,
    "attention": "scaled_dot",
    "train_data": {
        "22011": [1],
        "22014": [1],
        "22017": [1]
    },
    "validate_data": {
        "22011": [2],
        "22014": [2],
        "22017": [2]
    },
    "predict_data": {
        "22011": [2],
        "22014": [2],
        "22017": [2]
    },
    "sample_weights": [1, 1, 1],
    "gpu": [1],
    "optimizer": {
        "pct_start": 0.1,
        "lr_max": 1e-4,
        "div_factor": 25,
        "final_div_factor": 3e3,
        "betas": [0.9, 0.999],
        "eps": 1e-8,
        "weight_decay": 0,
        "amsgrad": true, 
        "base_momentum": 0.85,
        "max_momentum": 0.95,
        "anneal_strategy": "cos",
        "interval": "step",
        "frequency": 1
    }
}
